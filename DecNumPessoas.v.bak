module DecNumPessoas(
	input A,
	input B,
	input C,
	output [7:0] display
);
	assign display[0] = ~A; // A
	assign display[1] = ~A && ~B; // B
	assign display[2] = A && ~B; // c
	assign display[3] = ~A; // D
	assign display[4] = ~A || B; // E
	assign display[5] = A || B; // F
	assign display[6] = ~A; // G
	assign display[7] = 1; // P
endmodule
